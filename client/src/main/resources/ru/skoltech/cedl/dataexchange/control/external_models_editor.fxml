<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.glyphfont.Glyph?>
<fx:root stylesheets="@../view/styling.css" type="javafx.scene.control.ScrollPane"
         xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <HBox spacing="4.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <VBox fx:id="externalModelViewContainer" spacing="4.0">
                    <children>
                    </children>
                </VBox>
                <Button minWidth="28" mnemonicParsing="false" onAction="#addExternalModel" text="">
                    <tooltip>
                        <Tooltip text="Attach External Model"/>
                    </tooltip>
                    <graphic>
                        <Glyph fontFamily="FontAwesome" icon="PLUS"/>
                    </graphic>
                </Button>
            </children>
            <padding>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
            </padding>
        </HBox>
    </content>
</fx:root>
